{
  "name": "@furystack/example-repository",
  "version": "0.2.1",
  "description": "Content management layer for FuryStack",
  "main": "dist/index.js",
  "private": true,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "precommit": "npm run test",
    "commit": "git-cz",
    "test": "jest",
    "lint": "tslint --project tsconfig.json",
    "clean": "rimraf dist && rimraf coverage",
    "build": "tsc -b",
    "start": "node ./dist/index.js",
    "prepublishOnly": "npm run build",
    "publish:development": "npm run build && npm t && npm run typedoc && npm publish --tag development",
    "typedoc": "typedoc --mode file --out documentation src --tsconfig tsconfig.json --theme c:/Users/%USERNAME%/AppData/Roaming/npm/node_modules/@sensenet/typedoc-theme/sn-theme"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/furystack/content.git"
  },
  "keywords": [
    "FuryStack",
    "HTTP",
    "REST",
    "API",
    "Actions"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "gallay.lajos@gmail.com",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/furystack/content.git"
  },
  "homepage": "https://github.com/furystack/furystack",
  "dependencies": {
    "@furystack/auth-google": "^2.0.2",
    "@furystack/core": "^4.0.1",
    "@furystack/http-api": "^3.0.1",
    "@furystack/typeorm-store": "^1.0.3",
    "@furystack/odata": "^0.0.4",
    "@furystack/websocket-api": "^2.0.2",
    "sqlite": "3.0.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.9",
    "@types/uuid": "3.4.4",
    "jest": "24.1.0",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.0",
    "tslint": "5.13.0",
    "typescript": "^3.4.3"
  },
  "typings": "./dist/index.d.ts",
  "gitHead": "053ececc7dfe5a1ccc5f06c510b78b9999e1f4f0"
}
