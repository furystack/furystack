{
  "version": "0.0.2",
  "devDependencies": {
    "@types/terminal-kit": "^1.28.0",
    "@types/yargs": "^13.0.3",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsdoc": "^18.4.3",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.3",
    "lerna": "^3.19.0",
    "lint-staged": "^9.2.1",
    "prettier": "^1.17.1",
    "rimraf": "^3.0.0",
    "ts-json-schema-generator": "^0.58.0",
    "tslib": "^1.9.3",
    "typescript": "^3.7.3"
  },
  "scripts": {
    "build": "tsc -b",
    "lint": "eslint . --ext .tsx --ext .ts",
    "prettier": "prettier --write",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "start": "npm run build && node dist/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "name": "@furystack/onboard",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "src",
    "dist",
    "bin"
  ],
  "bin": {
    "onboard": "./bin/onboard"
  },
  "dependencies": {
    "@furystack/inject": "^4.0.4",
    "@furystack/logging": "^2.0.10",
    "@furystack/utils": "^1.1.0",
    "ajv": "^6.10.2",
    "semaphore-async-await": "^1.5.1",
    "terminal-kit": "^1.32.1",
    "yargs": "^15.0.2"
  }
}
