{
  "name": "@furystack/security",
  "version": "2.1.16",
  "description": "Security, password management and authorization related stuff for FuryStack",
  "type": "module",
  "scripts": {
    "fix:prettier": "prettier \"{,!(dist|temp)/**/}*.{ts,tsx}\" --write",
    "build:es6": "tsc --outDir ./esm",
    "build": "tsc -b"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "types": "./types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "types/*"
      ]
    }
  },
  "files": [
    "dist",
    "types",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/furystack/furystack.git"
  },
  "keywords": [
    "furystack",
    "security"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Gallay Lajos <gallay.lajos@gmail.com>",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/furystack/furystack/issues"
  },
  "dependencies": {
    "@furystack/core": "workspace:^",
    "@furystack/inject": "workspace:^",
    "@furystack/utils": "workspace:^"
  },
  "homepage": "https://github.com/furystack/furystack",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  }
}
