{
  "name": "@furystack/logging",
  "version": "1.1.3",
  "description": "Logging API for FuryStack package",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "precommit": "npm run test",
    "commit": "git-cz",
    "test": "rimraf coverage && jest",
    "lint": "tslint --project tsconfig.json",
    "clean": "rimraf dist && rimraf coverage",
    "build": "tsc -b",
    "prepublishOnly": "npm run build",
    "publish:development": "npm run build && npm t && npm run typedoc && npm publish --tag development",
    "typedoc": "typedoc --mode file --out documentation src --tsconfig tsconfig.json --theme c:/Users/%USERNAME%/AppData/Roaming/npm/node_modules/@sensenet/typedoc-theme/sn-theme"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/furystack/furystack.git"
  },
  "keywords": [
    "FuryStack",
    "Logging"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "gallay.lajos@gmail.com",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/furystack/furystack.git"
  },
  "nyc": {
    "exclude": [
      "temp/test/**/*.*",
      "temp/src/**/I*.js",
      "**/index.js"
    ],
    "include": "temp/src/**/*.*",
    "check-coverage": false,
    "cache": true,
    "all": true,
    "reporter": [
      "lcov",
      "text-summary",
      "json"
    ]
  },
  "homepage": "https://github.com/furystack/furystack.git",
  "dependencies": {
    "@furystack/inject": "^3.0.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "jest": "^24.7.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "config": {
    "commitizen": {
      "path": "sensenet-kfi-cz-conventional-changelog"
    }
  },
  "typings": "./dist/index.d.ts",
  "gitHead": "1045d854bfd8c475b7035471d130d401417a2321"
}
