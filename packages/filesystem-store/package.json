{
  "name": "@furystack/filesystem-store",
  "version": "7.0.35",
  "description": "Simple File System store implementation for FuryStack",
  "type": "module",
  "scripts": {
    "build": "tsc --outDir ./esm"
  },
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js"
    }
  },
  "files": [
    "esm",
    "src"
  ],
  "repository": {
