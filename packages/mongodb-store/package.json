{
  "name": "@furystack/mongodb-store",
  "version": "6.1.2",
  "description": "MongoDB Store implementation for FuryStack",
  "scripts": {
    "build:es6": "tsc --outDir ./esm"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "types": "./types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "types/*"
      ]
    }
  },
  "files": [
    "dist",
    "types",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/furystack/core.git"
  },
  "keywords": [
    "FuryStack",
    "Storage",
    "MongoDB",
    "NoSQL",
    "Document store"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "gallay.lajos@gmail.com",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/furystack/furystack/issues"
  },
  "homepage": "https://github.com/furystack/furystack",
  "dependencies": {
    "@furystack/core": "workspace:^",
    "@furystack/inject": "workspace:^",
    "@furystack/utils": "workspace:^",
    "mongodb": "5.2.0",
    "semaphore-async-await": "^1.5.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "typescript": "^5.0.4"
  },
  "gitHead": "1045d854bfd8c475b7035471d130d401417a2321"
}
