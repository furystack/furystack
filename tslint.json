{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "member-ordering": false,
        "object-literal-sort-keys": [
            false
        ],
        "max-line-length": false,
        "no-unused-expression": [
            true,
            "allow-fast-null-checks"
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "allow-leading-underscore"
        ]
    },
    "rulesDirectory": []
}